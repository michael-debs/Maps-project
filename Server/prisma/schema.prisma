
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  email String
  password String
  bio String?
  profilePicture String?
}

model Activity {
  id Int @id @default(autoincrement())
  name String
  description String
  profile String
}